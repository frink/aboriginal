KARCH=x86_64
KERNEL_PATH=arch/${KARCH}/boot/bzImage
GCC_FLAGS="--enable-sjlj-exceptions"
BINUTILS_FLAGS="--enable-64-bit-bfd"
QEMU_TEST=

ROOT=hda
CONSOLE=ttyS0

# Gentoo from Scratch
GFS_CHOST="x86_64-gentoo-linux-uclibc"
GFS_ARCH="amd64"

UCLIBC_CONFIG="
TARGET_x86_64=y
UCLIBC_HAS_FPU=y
"

emulator_command()
{
  echo qemu-system-x86_64 $(qemu_defaults "$@")
}
