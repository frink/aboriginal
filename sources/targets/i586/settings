KARCH=i386
KERNEL_PATH=arch/${KARCH}/boot/bzImage
BINUTILS_FLAGS=
GCC_FLAGS="--enable-sjlj-exceptions"
QEMU_TEST=$KARCH

ROOT=hda
CONSOLE=ttyS0

# Gentoo from Scratch
GFS_CHOST="i586-gentoo-linux-uclibc"
GFS_ARCH="x86"

UCLIBC_CONFIG="
TARGET_i386=y
CONFIG_586=y
UCLIBC_HAS_FPU=y
"

emulator_command()
{
  echo qemu -cpu pentium $(qemu_defaults "$@")
}
