DESCRIPTION="Standard 32-bit x86 target, Pentium 3 or later."

KARCH=i386
KERNEL_PATH=arch/${KARCH}/boot/bzImage
BINUTILS_FLAGS=
GCC_FLAGS=
QEMU_TEST=$KARCH

CONSOLE=ttyS0
ROOT=hda

UCLIBC_CONFIG="
ARCH_HAS_MMU=y
TARGET_i386=y
CONFIG_PENTIUMIII=y
UCLIBC_HAS_FPU=y
"

LINUX_CONFIG="
CONFIG_MPENTIUMII=y
CONFIG_ACPI=y
CONFIG_BLK_DEV_PIIX=y
CONFIG_NETDEV_1000=y
CONFIG_E1000=y
CONFIG_SERIAL_8250=y
CONFIG_SERIAL_8250_CONSOLE=y
"

emulator_command()
{
  echo qemu -cpu pentium3 $(qemu_defaults "$@") \
    -net nic,model=e1000 -net user
}
