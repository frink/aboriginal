KARCH=i386
KERNEL_PATH=arch/${KARCH}/boot/bzImage
BINUTILS_FLAGS=
GCC_FLAGS="--enable-sjlj-exceptions"
QEMU_TEST=$KARCH

CONSOLE=ttyS0
ROOT=hda

# Gentoo from Scratch
GFS_CHOST="i686-gentoo-linux-uclibc"
GFS_ARCH="x86"

UCLIBC_CONFIG="
ARCH_HAS_MMU=y
TARGET_i386=y
CONFIG_PENTIUMIII=y
UCLIBC_HAS_FPU=y
"

emulator_command()
{
  echo qemu -cpu pentium3 $(qemu_defaults "$@") \
    -net nic,model=e1000 -net user
}
