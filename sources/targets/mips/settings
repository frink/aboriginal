KARCH=mips
KERNEL_PATH=vmlinux
BINUTILS_FLAGS=
GCC_FLAGS=
QEMU_TEST=mips

ROOT=hda
CONSOLE=ttyS0

# Gentoo from Scratch
GFS_CHOST="mips-gentoo-linux-uclibc"
GFS_ARCH="mips"

UCLIBC_CONFIG="
TARGET_mips=y
UCLIBC_HAS_FPU=y
"

LINUX_CONFIG="
CONFIG_MIPS_MALTA=y
CONFIG_CPU_MIPS32_R2=y
CONFIG_SERIAL_8250=y
CONFIG_SERIAL_8250_CONSOLE=y
#CONFIG_PM=y
CONFIG_PCNET32=y
CONFIG_BLK_DEV_PIIX=y
"

emulator_command()
{
  echo qemu-system-mips -M malta $(qemu_defaults "$@")
}
