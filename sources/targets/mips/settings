KARCH=mips
KERNEL_PATH=vmlinux
BINUTILS_FLAGS=
GCC_FLAGS="--enable-sjlj-exceptions"
QEMU_TEST=mips

ROOT=hda
CONSOLE=ttyS0

# Gentoo from Scratch
GFS_CHOST="mips-gentoo-linux-uclibc"
GFS_ARCH="mips"

UCLIBC_CONFIG="
TARGET_mips=y
UCLIBC_HAS_FPU=y
"

emulator_command()
{
  echo qemu-system-mips -M malta $(qemu_defaults "$@")
}
