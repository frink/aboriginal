+++ linux2/arch/arm/mach-versatile/Kconfig	2009-12-08 04:29:50.000000000 -0600
@@ -3,14 +3,12 @@
 
 config ARCH_VERSATILE_PB
 	bool "Support Versatile/PB platform"
