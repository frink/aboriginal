+++ uClibc.bak/Makefile.in	2012-10-08 18:52:41.697432235 -0500
@@ -195,7 +195,7 @@
 	HOSTCC="$(HOSTCC)" \
 	install
 
-install: install_runtime install_dev
+install: install_headers install_runtime install_dev
 
 
